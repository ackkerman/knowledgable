{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "tui",
  "tasks": {
    "docs-sync": { "outputs": [] },

    "build": {
      "dependsOn": ["^build", "docs-sync"],
      "outputs": ["dist/**"]
    },
    "dev": {
      "dependsOn": ["^dev"],
      "persistent": true,
      "cache": false,
      "outputs": []
    },
    "lint": {},
    "test": {}
  }
}